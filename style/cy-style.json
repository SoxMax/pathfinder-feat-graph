[
  {
    "selector": "node",
    "style": {
      "label": "data(name)",
      "color": "#fff",
      "text-outline-color": "#888",
      "text-outline-width": 1,
      "text-valign": "center",
      "text-halign": "center",
      "text-max-width": 100,
      "text-justification": "center",
      "text-wrap": "wrap",
      "shape": "ellipse",
      "height": 40,
      "width": 80,
      "display": "none"
    }
  },
  {
    "selector": "edge",
    "style": {
      "curve-style": "straight",
      "target-arrow-shape": "triangle-backcurve",
      "events": "no",
      "line-cap": "round"
    }
  },
  {
    "selector": ".visible",
    "style": {
      "display": "element"
    }
  },
  {
    "selector": ":selected",
    "style": {
      "background-color": "#0d6efd",
      "text-outline-color": "#0261ed"
    }
  }
]